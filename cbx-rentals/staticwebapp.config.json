{
  "navigationFallback": {
    "rewrite": "/index.html",
    "filter": "ALL"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  }
}